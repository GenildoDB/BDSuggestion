<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BDSuggestion.View.FlyoutNave"
             xmlns:pages="clr-namespace:BDSuggestion.View">
  <FlyoutPage.Flyout>
    <pages:FlyoutNaveFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage x:Name="Navi" x:FieldModifier="public" BarBackgroundColor="{DynamicResource Fundo}" BarTextColor="White">
      <x:Arguments>
        <pages:Home />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>